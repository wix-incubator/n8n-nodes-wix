{
	"name": "@wix/n8n-nodes-wix",
	"version": "1.0.3",
	"description": "Wix triggers & actions integration for n8n",
	"license": "MIT",
	"homepage": "",
	"keywords": [
		"n8n-community-node-package"
	],
	"author": {
		"name": "Itai Ben David",
		"email": "itaibd@wix.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/wix-incubator/n8n-nodes-wix.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "n8n-node prerelease"
