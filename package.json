			"dist/nodes/Wix/Wix.node.js",
			"dist/nodes/Wix/WixTrigger.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"@types/node": "^24.10.1",
		"@wix/ecom": "^1.0.1501",
		"@wix/stores": "^1.0.619",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	},
	"packageManager": "yarn@4.12.0"
